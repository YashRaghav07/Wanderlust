<%if(success&&success.length){%>
<div
  class="alert alert-success alert-dismissible col-8 offset-2 mt-3"
  role="alert"
>
  <%=success%>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%> <%if(failure&&failure.length){%>
<div
  class="alert alert-danger alert-dismissible col-8 offset-2 mt-3"
  role="alert"
>
  <%=failure%>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<%}%>
